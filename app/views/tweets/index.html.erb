 <h1>ブログ一覧</h1>

<p><%= notice %></p>
<table>
  <tr>
    <th>コンテンツ</th>
  </tr>

<% @tweet.each do |tweet| %>
  <tr>
    <td><%= tweet.content %></td>
    <td><%= link_to '詳細', tweet_path(tweet.id)  %></td>
    <td><%= link_to '編集', edit_tweet_path(tweet.id),data: { confirm: '本当に編集していいですか？' } %></td>
    <td><%= link_to '削除', tweet_path(tweet.id), method: :delete, data: { confirm: '本当に削除していいですか？' } %></td>
  </tr>
<% end %>
</table>

